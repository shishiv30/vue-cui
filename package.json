{
    "name": "vue-cui",
    "version": "1.0.0",
    "description": "a UI solution base on vue and cui",
    "main": "index.js",
    "scripts": {
        "client-dev": "webpack-dev-server --config config/webpack.client.config.js --mode=development --open",
        "client-pro": "webpack-dev-server --config config/webpack.client.config.js --mode=production --open",
        "server-dev": "webpack --config config/webpack.server.config.js --mode=development && node index.js",
        "server-pro": "webpack --config config/webpack.client.config.js --mode=production && webpack --config config/webpack.server.config.js --mode=production && node index.js",
        "deploy": "npm run server-pro && node publish.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shishiv30/vue-cui.git"
    },
    "keywords": [
        "vue",
        "cui",
        "ui/ux"
